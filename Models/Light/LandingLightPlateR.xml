<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  <path>LandingLightPlate.ac</path>

  <animation>
    <type>select</type>
    <object-name>LHLNDlight</object-name>
    <condition>
      <and>
        <property>sim/multiplay/generic/int[18]</property>
        <greater-than>
          <property>systems/electrical/volts</property>
          <value>8</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>material</type>
    <object-name>LHLNDlight</object-name>
    <emission>
      <red>   0.027 </red>
      <green> 0.027 </green>
      <blue>  0.027 </blue>
      <factor-prop>systems/electrical/outputs/landing-lights[1]</factor-prop>
    </emission>
  </animation>

</PropertyList>
